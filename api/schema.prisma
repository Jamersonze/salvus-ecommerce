datasource db {
  provider = "postgresql"
  url      = env("SUPABASE_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Produto {
  id       Int      @id @default(autoincrement())
  name     String
  description    String?
  price     Float?   @default(0.0)
  created_at    DateTime?  @default(now())
}